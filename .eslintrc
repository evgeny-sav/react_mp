{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "prettier", "react"
  ],
  "extends": [
    "prettier", "eslint:recommended", "plugin:react/recommended"
  ],
  "rules": {
    "prettier/prettier": ["error", "fb"],
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"]
  }
}
