{
  "name": "react_mp",
  "version": "1.0.0",
  "description": "ReactJS Mentoring Programm",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config webpack.dev.js && nodemon server.js",
    "test": "jest -u",
    "webpack-dev-server": "webpack-dev-server  --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:server": "cross-env NODE_ENV=server webpack --config webpack.server.js",
    "serve": "nodemon server.js",
    "serve:dist": "nodemon dist/server.js",
    "eslint": "eslint **/*.{jsx,js}",
    "eslint:fix": "eslint --fix **/*.{jsx,js}",
    "stylelint": "stylelint **/*.scss",
    "stylelint:fix": "stylelint **/*.scss --syntax scss --fix",
    "precommit": "lint-staged"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/testData/shim.js",
      "<rootDir>/testData/tests.setup.js"
    ]
  },
  "lint-staged": {
    "**/*.scss": "stylelint --fix",
    "*.{jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evgeny-sav/react_mp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evgeny-sav/react_mp/issues"
  },
  "homepage": "https://github.com/evgeny-sav/react_mp#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.2",
    "jest": "^21.2.1",
    "nodemon": "^1.11.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.0.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.1.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.5.5",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "fake-style-loader": "^1.0.1",
    "file-loader": "^1.1.5",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^4.2.1",
    "node-sass": "^4.5.3",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.1.1",
    "stylelint-config-recommended": "^1.0.0",
    "webpack-dev-server": "^2.7.1"
  }
}
